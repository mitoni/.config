alt - k : yabai -m window --focus south
alt - i : yabai -m window --focus north
alt - j : yabai -m window --focus west
alt - l : yabai -m window --focus east

alt - k : yabai -m window --warp south
alt - i : yabai -m window --warp north
alt - j : yabai -m window --warp west
alt - l : yabai -m window --warp east

alt - f : yabai -m window --toggle zoom-fullscreen
alt - b : yabai -m space --balance
